{
  "name": "squidback",
  "version": "1.0.0",
  "description": "unleash your feedbacks",
  "main": "index.js",
  "scripts": {
    "build-main": "browserify src/main.js -o www/main.js",
    "build-audio-worklet": "browserify src/worklet/phase-vocoder.js -o www/phase-vocoder.js",
    "watch-main": "watchify src/main.js -o www/main.js --debug --verbose ",
    "watch-audio-worklet": "watchify src/worklet/phase-vocoder.js -o www/phase-vocoder.js --debug --verbose",
    "build": "npm run build-main | npm run build-audio-worklet",
    "watch": "npm run watch-main | npm run watch-audio-worklet",
    "start": "http-server -a 0.0.0.0 -S www/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "feedback",
    "larsen",
    "performance",
    "instrument",
    "adaptive filter"
  ],
  "author": "Gianluca Elia",
  "license": "",
  "dependencies": {
    "array-smooth": "^1.0.0",
    "cubic-spline": "^3.0.3",
    "d3-peaks": "0.0.1",
    "fft.js": "^4.0.3",
    "http-server": "^0.12.1",
    "kissfft-js": "^0.1.8",
    "rtcmulticonnection": "^3.7.0",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "browserify": "^16.5.0",
    "watchify": "^3.11.1"
  }
}
